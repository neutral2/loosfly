}function pageCon(opt){
	var maxPage = "22";
	if( opt == 'next' ){
		if( nnp < maxPage ) nnp += 1;
		else{ 
			alert('마지막 페이지입니다.');
		}
	}else{
		if( nnp > 1 ) nnp -= 1;
		else alert('첫 페이지입니다.');
	}
	
	pageMove(nnp);
	
}function pageMove(page){
	nnp = page;
	var bl = nnp/10;
	if( (nnp%10) == 0 ) bl = eval(bl) - 1;
	blockNo = parseInt(bl);
	pageing();
	skinDisplays(nnp);
}function pageing(){
	var maxPage = "22";
	var blockObj = document.getElementById('page');
	var block = 0;
	var listNo = 0;
	var Npage = 0;
	var pageStr = new Array();
	var pageInt = new Array();
	pageStr['0'] = new Array();
	pageInt['0'] = new Array();
	for( p=0; p<maxPage;p++ ){
		if( p%10==0 && p!=0 ){
			block+=1;
			pageStr[block]=new Array();
			pageInt[block]=new Array();
			listNo = 0;
		}		if( (p+1) == nnp ) var bold_st = "font-weight:bold;font-size:9px;";
		else var bold_st = "font-size:9px;";
		pageStr[block][listNo] = "<span style='padding:0 2px 0 2px;cursor:pointer;"+bold_st+"' id='page_"+(p+1)+"' onclick='pageMove("+(p+1)+")'>"+(p+1)+"</span>";
		pageInt[block][listNo] = (p+1);
		listNo+=1;
	}
	
	var next_blockNo = eval(blockNo)+1;
	var pageingString = '';	pageingString += '<span>';
	for( i=0; i<pageStr[blockNo].length; i++ ){
		pageingString += pageStr[blockNo][i];
	}
	pageingString += '</span>';
	if( pageInt[next_blockNo] ){ 
		var nextStr = "<span style='padding-left:3px;cursor:pointer;' onclick='pageMove("+pageInt[next_blockNo]['0']+");'>[Next]</span>";
		var prevStr = "";
	}else{ 
		var nextStr = "";
		var prevNo = eval(pageInt[blockNo]['0']) - 1;
		var prevStr = "<span style='padding-right:3px;cursor:pointer;' onclick='pageMove("+prevNo+");'>[Prev]</span>";
	}	var print_ps = prevStr+''+pageingString+''+nextStr;	blockObj.innerHTML = print_ps;
}
pageing();
</script>